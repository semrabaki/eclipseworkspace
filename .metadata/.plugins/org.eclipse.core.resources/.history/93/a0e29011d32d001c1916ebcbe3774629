package reviewquestionspage901;

public class Q2 {
	
	//Type java code by using while loop. 
	//Write a program that types first 30 consecutive odd integers.

	public static void main(String[] args) {
		
		int i=0;
		
		
		while(i<=60)
		{
			if(i%2==0)
			{
				System.out.println(i);
			}
			
		}

	}

}
